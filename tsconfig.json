{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "jsxImportSource": "solid-js",
        "jsx": "preserve",
        "strict": true,
        "skipLibCheck": true,
        "types": [
            "solid-start/env",
            "vite-plugin-pwa/solid",
            "vite-plugin-pwa/info"
        ],
        "baseUrl": "./",
        "paths": {
            "~/*": [
                "./src/*"
            ]
        },
        // "verbatimModuleSyntax": true
    }
}